    .head h2{font-size:18px;font-weight:700}
    .sub{font:600 11px 'Inter';color:var(--muted);letter-spacing:.12em;text-transform:uppercase}

    /* Panels + lists */
    .panel{background:var(--panel);border:1px solid var(--line);padding:18px}
    .list{margin-left:16px}
    .list li{margin:6px 0}
    .mono{font:12px "Courier New",ui-monospace,SFMono-Regular,Menlo,Consolas,monospace;color:var(--muted)}

    /* Cards grid */
    .grid{display:grid;grid-template-columns:repeat(auto-fill,minmax(260px,1fr));gap:18px}
    .card{background:var(--panel);border:1px solid var(--line);padding:18px;display:flex;flex-direction:column;gap:8px;transition:transform .12s ease,box-shadow .12s ease}
    .card:hover{transform:translateY(-2px);box-shadow:0 4px 18px rgba(0,0,0,.08)}
    .card h3{font:700 15px 'Inter';margin-bottom:2px}
    .card p{font:13px 'Inter';color:var(--soft)}
    .pills{display:flex;flex-wrap:wrap;gap:8px;margin-top:8px}
    .pill{display:inline-block;font:700 11px 'Inter';letter-spacing:.06em;text-transform:uppercase;padding:4px 8px;border-radius:6px;border:1px solid var(--line);background:#fff;color:var(--soft)}
    .pill.warn{background:#fff1f2;border-color:#fecdd3;color:#7f1d1d}
    .pill a{color:inherit;text-decoration:none} /* keep pill text color */

    /* Footer */
    footer{border-top:1px solid var(--line);margin-top:48px;padding:18px 0;color:var(--muted);font:13px 'Inter'}
